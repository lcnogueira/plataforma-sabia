{
  "name": "adonis-api-app",
  "version": "4.1.0",
  "adonis-version": "4.1.0",
  "description": "Adonisjs boilerplate for API server with pre-configured JWT",
  "main": "index.js",
  "scripts": {
    "dev": "adonis serve --dev --debug",
    "start:ci": "NODE_ENV=testing node server.js",
    "start": "node server.js",
    "test": "node ace test",
    "test:unit": "node ace test unit",
    "test:functional": "node ace test functional",
    "coverage": "nyc npm run test",
    "algolia:index": "node ace algolia:index --override --settings",
    "migration:run": "node ace migration:run",
    "migration:refresh": "node ace migration:refresh",
    "seed": "node ace seed",
    "seed:default": "node ace seed --files=\"00_RoleSeeder.js, 02_TaxonomySeeder.js, 03_TermSeeder.js, 05_PermissionSeeder.js, 10_DisclaimerSeeder.js\"",
    "apidoc": "apidoc -e \"(node_modules|public)\" -o public/apidoc"
  },
  "keywords": [
    "adonisjs",
    "adonis-app"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/antl": "^2.0.7",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.5",
    "@adonisjs/cli": "^4.0.12",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.9",
    "@adonisjs/generic-exceptions": "^3.0.1",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/mail": "^3.0.10",
    "@adonisjs/validator": "^5.0.6",
    "@adonisjs/view": "^2.0.1",
    "@adonisjs/vow": "^1.0.17",
    "@rocketseat/adonis-bull": "^0.2.2",
    "algoliasearch": "^4.1.0",
    "apidoc": "^0.24.0",
    "cli-progress": "^3.6.0",
    "dayjs": "^1.8.24",
    "mysql2": "^2.1.0",
    "rand-token": "^1.0.1",
    "sinon": "^9.0.1",
    "slugify": "^1.4.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "nyc": "^15.0.0"
  },
  "autoload": {
    "App": "./app"
  }
}